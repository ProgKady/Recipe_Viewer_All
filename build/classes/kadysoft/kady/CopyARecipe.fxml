      </JFXTextField>
      <JFXTextField fx:id="des" alignment="CENTER" labelFloat="true" layoutX="175.0" layoutY="199.0" prefHeight="37.0" prefWidth="661.0" promptText="Recipe To Paste ..." style="-fx-background-radius: 3em;">
